templateFile = open('template57BRAM.coe', 'w')
templateFile.write('memory_initialization_radix=2;\n')
templateFile.write('memory_initialization_vector=\n')

searchFile = open('search57BRAM.coe', 'w')
searchFile.write('memory_initialization_radix=2;\n')
searchFile.write('memory_initialization_vector=\n')

size = len(template) - 1

for i in template:
    templateFile.write(bin(i)[2:].zfill(8))
    if i == size:
        templateFile.write(';\n')
    else:
        templateFile.write(',\n')

size = len(search) - 1

